{
    "name": "coffin-laughter/jwt-auth",
    "description": "thinkphp  jwt auth composer",
    "type": "library",
    "require": {
        "php": "^7.0",
        "lcobucci/jwt": "4.2.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "coffin_laughter",
            "email": "chuanshouo_yongyuan@163.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "coffin\\jwtauth\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "coffin\\jwtauth\\Service"
            ],
            "config": {
                "jwt": "config/config.php"
            }
        }
    },
    "minimum-stability": "dev"
}
